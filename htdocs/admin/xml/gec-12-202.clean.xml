<?xml version="1.0"?>
<law>
  <structure>
    <unit label="article" identifier="gec" order_by="gec" level="1">Economic Development</unit>
  </structure>
  <section_number>gec-12-202</section_number>
  <catch_line>This subtitle is self-executing....</catch_line>
  <order_by>202</order_by>
  <text>
    <section prefix="(a)">
      <section prefix="(1)">This subtitle is self-executing.</section>
      <section prefix="(2)">A political subdivision need not amend its charter to exercise the powers granted by this subtitle.</section>
    </section>
    <section prefix="(b)">This subtitle does not apply in Baltimore City.</section>
  </text>
</law>
