<?xml version="1.0"?>
<law>
  <structure>
    <unit label="article" identifier="gbr" order_by="gbr" level="1">Business Regulation</unit>
  </structure>
  <section_number>gbr-1-301</section_number>
  <catch_line>In this subtitle, "business record" includes:...</catch_line>
  <order_by>301</order_by>
  <text>In this subtitle, "business record" includes:<section prefix=""><section prefix="(1)">a book of account;</section><section prefix="(2)">a canceled check;</section><section prefix="(3)">a document;</section><section prefix="(4)">a letter;</section><section prefix="(5)">a payroll;</section><section prefix="(6)">a production report or other record that relates to equipment, personnel, or sales; and</section><section prefix="(7)">a voucher.</section></section></text>
</law>
